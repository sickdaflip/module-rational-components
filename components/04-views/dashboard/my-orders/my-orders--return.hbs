<div class="page-wrapper">
    {{ render '@header' }}

    <div class="dashboard container">
        <div class="row">
            <div class="col-xs-12 col-md-4 col-lg-3">
                {{ render '@dashboard-nav' }}
            </div>

            <div class="col-xs-12 col-md-8 col-lg-9">
                <h2 class="heading heading--fourth-level dashboard__title">
                    {{ title }}
                </h2>

                <div class="row">
                    {{> '@information' info }}
                </div>

                {{> '@input' contactEmailAddress }}

                {{> '@dashboard-form' form }}

                {{> '@action' addReturnItem }}

                {{> '@input--textarea' comments }}

                {{> '@action' action }}
            </div>
        </div>
    </div>
</div>

{{ render '@footer' }}
